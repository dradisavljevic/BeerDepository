<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.beer.depository">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/can_launcher_square"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/can_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.beer.depository.activity.CatalogueActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.beer.depository.activity.CanDetailActivity"
            android:label="@string/title_can_detail"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>